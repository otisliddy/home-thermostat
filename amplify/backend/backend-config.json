{
  "storage": {
    "deviceState": {
      "service": "DynamoDB",
      "providerPlugin": "awscloudformation"
    }
  },
  "function": {
    "homethermostatChangeState": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "storage",
          "resourceName": "deviceState",
          "attributes": [
            "Name",
            "Arn",
            "StreamArn"
          ]
        }
      ]
    }
  },
  "custom": {
    "scheduleStateChange": {
      "service": "customCloudformation",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "homethermostatChangeState",
          "attributes": [
            "Name",
            "Arn",
            "Region",
            "LambdaExecutionRole"
          ]
        }
      ]
    }
  }
}